<div class="div container mt-3">
    <div class="row">
        <div class="col-md-8">
            <h2>Detail Data Siswa</h2>
            <div class="card " style="width: 18rem;">
                <div class="card-body ">
                  <h5 class="card-title"><%= student.nama.toUpperCase() %></h5>
                  <h6 class="card-subtitle mb-2 text-body-secondary"><%= student.nisn %></h6>
                  <p class="card-text"><%= student.email%></p>
                  <p class="card-text"><%= student.phone%></p>
                  <p class="card-text"><%= student.address%></p>
                </div>
                 <a href="/" class="btn btn-primary">Update</a>
                <a href="/" class="btn btn-danger">Delete</a> <br>
                <a href="/student" class="card-link">&laquo;Kembali Ke Halaman Dashboard</a>
              </div>
        </div>
    </div>
</div>